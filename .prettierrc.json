{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "htmlWhitespaceSensitivity": "css",
    "proseWrap": "preserve",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto"
  }
  